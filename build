export ARCH=arm64
export SUB_ARCH=arm64
export PATH=/mnt/Building/aarch64-linux-gnu-4.x-kernel-pure-linaro/bin/:$PATH
export CROSS_COMPILE=aarch64-linux-gnu-

if [ -e out/arch/arm64/boot ]; then
	rm -rf out/arch/arm64/boot
fi

export KBUILD_DIFFCONFIG=dora_diffconfig
make O=out mrproper
make O=out clean
make O=out mrproper
make O=out msm-perf_defconfig
make O=out -j16

echo "checking for compiled kernel..."
if [ -f out/arch/arm64/boot/Image.gz-dtb ]
then

	echo "DONE"
fi
